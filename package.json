    "@babel/preset-typescript": "^7.15.0",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.1.0",
